{
  "name": "SharedDeposit",
  "license": "BUSL-1.1",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-solhint": "^3.0.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/jest": "^29.5.8",
    "@types/mocha": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "hardhat": "^2.19.1",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.43",
    "hardhat-deploy-ethers": "^0.4.1",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-preprocessor": "^0.1.5",
    "hardhat-storage-layout": "^0.1.7",
    "hardhat-waffle": "^0.0.1-security",
    "hardhat-watcher": "^2.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prettier-plugin-solidity": "^1.2.0",
    "solhint": "^4.0.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fei-protocol/erc4626": "^0.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.1",
    "ethers": "^6.13.1",
    "solmate": "^6.7.0"
  },
  "scripts": {
    "uml": "npx sol2uml ./flats",
    "flat": "npx hardhat flat contracts/${CNAME} --output flats/${CNAME}",
    "flatten": "npx hardhat flatten --remove-licenses ./contracts/sharedDeposit_v2.sol > flattened/sharedDeposit_v2_flat.sol",
    "build": "npm run-script prettier ; npm run-script prettier:check ; npm run-script lint:sol ; npx hardhat compile",
    "clean": "hardhat clean",
    "clean_logs": "rm deploy_log.json && rm log.txt && rm verify_attempt_log.json",
    "clean_all": "npm run-script clean && npm run-script clean_logs",
    "rebuild": "npm run-script clean_all && npm run-script build",
    "commit": "git-cz",
    "compile": "hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.js\"",
    "deploy_v2_goerli": "npx hardhat run --network goerli deploy/deploy_v2.js",
    "deploy_v2_goerli_showall": "npx hardhat run --network goerli --verbose --show-stack-traces deploy/deploy_v2.js",
    "deploy_gov": "npx hardhat run deploy/deploy_gov.js",
    "deploy_gov_goerli": "npx hardhat run --network goerli deploy/deploy_gov.js",
    "deploy_gov_prod": "npx hardhat build && npx hardhat clean && npx hardhat run --network mainnet deploy/deploy_gov.js",
    "deploy": "hardhat deploy:Greeter",
    "sol": "npm run-script prettier ; npm run-script lint:sol ; npx hardhat compile",
    "sol:fix": "solhint --config .solhint.json --max-warnings 50 \"contracts/v2/**/*.sol\" --fix",
    "lint": "yarn run lint:sol && yarn run lint:ts && yarn run prettier:check",
    "lint:sol": "solhint --config .solhint.json --max-warnings 50 \"contracts/v2/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "test": "hardhat test",
    "loc:sol:setup": "brew install cloc",
    "loc:sol": "cloc contracts/v2/"
  }
}
